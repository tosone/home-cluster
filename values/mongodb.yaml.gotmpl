persistence:
  enabled: true
  storageClass: {{ .Values | get "storageClass" "local-path" }}
  size: 1Gi

auth:
  enabled: true
  rootPassword: {{ .Values | get "password" "secret" }}
  username: {{ .Values | get "username" "tosone" }}
